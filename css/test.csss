html {
  font-family: 'Merriweather', serif;
  font-size: 16px;
  color: rgba(0,0,0,0.8);
  line-height: 1.65;
}


/* Headings */
h1, h2, h3, h4, h5, h6 {
  font-family: 'Lato', sans-serif;
  font-weight: 400;
  margin-bottom: .5rem;
  line-height: 1.25;
  color: #313131;
  text-rendering: optimizeLegibility;
}